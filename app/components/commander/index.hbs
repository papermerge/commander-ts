<Breadcrumb
    @items={{this.breadcrumb}}
    @isLoading={{this.load.isLoading}}
    @onClick={{@onNodeClick}} />


{{#each this.nodes as |node|}}
    {{#let (component node.nodeType) (eq this.loading_uuid node.id) as |NodeType is_loading|}}
        <NodeType
            @node={{node}}
            @onClick={{@onNodeClick}}
            @is_loading={{is_loading}} />
    {{/let}}
{{else}}
    Empty
{{/each}}
