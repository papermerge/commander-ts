<Breadcrumb @endpoint={{@endpoint}} @onClick={{@onNodeClick}} />

{{#if this.load.isResolved}}
    {{#each this.load.value as |node|}}
        {{#let (component node.nodeType) as |NodeType|}}
            <NodeType @node={{node}} @onClick={{@onNodeClick}} />
        {{/let}}
    {{else}}
        Empty
    {{/each}}
{{else if this.load.isLoading}}
  Loading...
{{else if this.load.isError}}
    {{ this.load.error }}
{{/if}}
